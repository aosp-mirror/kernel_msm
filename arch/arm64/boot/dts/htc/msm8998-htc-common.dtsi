#include "msm8998-audio-muskie.dtsi"
#include "msm8998-htc-fingerprint-muskie.dtsi"

&i2c_6 {
	status = "disabled";
};

&i2c_7 {
	status = "okay";
};

&i2c_8 {
	status = "okay";
};

&i2c_9 {
	status = "okay";
};

&i2c_5_active {
	config {
		drive-strength = <8>;
	};
};
&i2c_5_sleep {
	config {
		drive-strength = <8>;
	};
};

