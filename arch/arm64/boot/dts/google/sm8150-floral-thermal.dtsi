			};
		};
	};

	pm8150l-vph-lvl0 {
		disable-thermal-zone;
		cooling-maps {
			vph_cpu4 {
				trip = <&vph_lvl0>;
				cooling-device =
					<&CPU4 10 10>;
			};
			vph_cpu5 {
				trip = <&vph_lvl0>;
				cooling-device =
					<&CPU5 10 10>;
			};
			vph_gpu0 {
				trip = <&vph_lvl0>;
				cooling-device = <&msm_gpu 2 2>;
			};
		};
	};

	pm8150l-vph-lvl1 {
		disable-thermal-zone;
		cooling-maps {
			vph_cpu6 {
				trip = <&vph_lvl1>;
				cooling-device =
