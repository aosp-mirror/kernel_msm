&thermal_zones {
	skin-therm-cpu {
		polling-delay-passive = <5000>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM1_PU2>;
		wake-capable-sensor;
		trips {
			skin_cpu_light: skin-cpu-light {
				temperature = <37000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_cpu_moderate_pre_lv1: skin-cpu-moderate-pre-lv1 {
				temperature = <39000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_cpu_moderate: skin-cpu-moderate {
				temperature = <41000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_cpu_severe: skin-cpu-severe {
				temperature = <43000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_cpu_critical: skin-cpu-critical {
				temperature = <45000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
		cooling-maps {
			/* light throttling */
			cpu6-turbo {
				trip = <&skin_cpu_light>;
				cooling-device =
					<&CPU6 1 2>;
			};
			cpu7-turbo {
				trip = <&skin_cpu_light>;
				cooling-device =
					<&CPU7 1 1>;
			};
			/* moderate pre level 1 throttling */
			cpu6-nominal-l1 {
				trip = <&skin_cpu_moderate_pre_lv1>;
				cooling-device =
					<&CPU6 3 3>;
			};
			cpu7-nominal-l1 {
				trip = <&skin_cpu_moderate_pre_lv1>;
				cooling-device =
					<&CPU7 2 3>;
			};
			/* moderate throttling */
			cpu0-nominal {
				trip = <&skin_cpu_moderate>;
				cooling-device =
					<&CPU0 1 3>;
			};
			cpu6-nominal {
				trip = <&skin_cpu_moderate>;
				cooling-device =
					<&CPU6 4 4>;
			};
			cpu7-nominal {
				trip = <&skin_cpu_moderate>;
				cooling-device =
					<&CPU7 4 4>;
			};
			/* severe throttling */
			cpu0-svs-l1 {
				trip = <&skin_cpu_severe>;
				cooling-device =
					<&CPU0 4 4>;
			};
			cpu6-svs-l1 {
				trip = <&skin_cpu_severe>;
				cooling-device =
					<&CPU6 5 5>;
			};
			cpu7-svs-l1 {
				trip = <&skin_cpu_severe>;
				cooling-device =
					<&CPU7 5 5>;
			};
			/* critical throttling */
			cpu0-svs {
				trip = <&skin_cpu_critical>;
				cooling-device =
					<&CPU0 5 5>;
			};
			cpu6-svs {
				trip = <&skin_cpu_critical>;
				cooling-device =
					<&CPU6 6 6>;
			};
			cpu7-svs {
				trip = <&skin_cpu_critical>;
				cooling-device =
					<&CPU7 6 6>;
			};
		};
	};

	skin-therm {
		polling-delay-passive = <5000>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM1_PU2>;
		wake-capable-sensor;
		trips {
			skin_light_pre_lv1: skin-light-pre-lv1 {
				temperature = <36000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_light: skin-light {
				temperature = <37000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_moderate_pre_lv1: skin-moderate-pre-lv1 {
				temperature = <39000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_moderate: skin-moderate {
				temperature = <41000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_severe: skin-severe {
				temperature = <43000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_critical: skin-critical {
				temperature = <45000>;
				hysteresis = <1000>;
				type = "passive";
			};
			skin_emergency_pre_lv1: skin-emergency-pre-lv1 {
				temperature = <48000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
		cooling-maps {
			/* light pre level 1 throttling */
			wired-charge-ibat-3500mA {
				trip = <&skin_light_pre_lv1>;
				cooling-device =
					<&google_charger 1 1>;
			};
			/* light throttling */
			wired-charge-ibat-3000mA {
				trip = <&skin_light>;
				cooling-device =
					<&google_charger 2 2>;
			};
			/* moderate pre level 1 throttling */
			wired-charge-ibat-2500mA {
				trip = <&skin_moderate_pre_lv1>;
				cooling-device =
					<&google_charger 3 3>;
			};
			wireless-charge-iusb-500mA {
				trip = <&skin_moderate_pre_lv1>;
				cooling-device =
					<&google_wlc_charger 1 1>;
			};
			/* moderate throttling */
			wired-charge-ibat-1500mA {
				trip = <&skin_moderate>;
				cooling-device =
					<&google_charger 5 5>;
			};
			/* severe throttling */
			wired-charge-ibat-500mA {
				trip = <&skin_severe>;
				cooling-device =
					<&google_charger 7 7>;
			};
			wireless-charge-iusb-110mA {
				trip = <&skin_severe>;
				cooling-device =
					<&google_wlc_charger 2 2>;
			};
			/* critical throttling */
			wired-charge-disabled {
				trip = <&skin_critical>;
				cooling-device = <&google_charger
					THERMAL_MAX_LIMIT THERMAL_MAX_LIMIT>;
			};
			wireless-charge-disabled {
				trip = <&skin_critical>;
				cooling-device = <&google_wlc_charger
					THERMAL_MAX_LIMIT THERMAL_MAX_LIMIT>;
			};
			gpu-svs-l1 {
				trip = <&skin_critical>;
				cooling-device =
					<&msm_gpu 1 1>;
			};
			/* emergency pre level1 throttling */
			cpu0-svs {
				trip = <&skin_emergency_pre_lv1>;
				cooling-device =
					<&CPU0 5 5>;
			};
			cpu6-svs {
				trip = <&skin_emergency_pre_lv1>;
				cooling-device =
					<&CPU6 6 6>;
			};
			cpu7-svs {
				trip = <&skin_emergency_pre_lv1>;
				cooling-device =
					<&CPU7 6 6>;
			};
		};
	};

