&pm7250b_vadc {
	disp_therm@4e {
		reg = <ADC_AMUX_THM2_PU2>;
		label = "disp_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	mmw_side_therm@53 {
		reg = <ADC_GPIO2_PU2>;
		label = "mmw_side_therm";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

