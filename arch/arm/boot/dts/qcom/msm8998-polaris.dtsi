		qca,bt-vdd-pa-supply = <&pm8998_l17_pin_ctrl>;
		qca,bt-vdd-ldo-supply = <&pm8998_l25_pin_ctrl>;
		qca,bt-chip-pwd-supply = <&pmi8998_bob_pin1>;

