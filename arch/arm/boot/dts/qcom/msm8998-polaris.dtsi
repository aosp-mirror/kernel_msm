	};

	tmp108@4a {
		/* U34 - Near PMI8998 */
		compatible = "ti,tmp108";
		reg = <0x4a>;
	};

	tmp108@4b {
		/* U35 - Near UFS */
		compatible = "ti,tmp108";
		reg = <0x4b>;
	};
};

